{
    "name": "bezhansalleh/laravel-plans",
    "description": "Laravel Plans is a package for SaaS apps that need management over plans, features, subscriptions, events for plans or limited, countable features.",
    "keywords": [
        "eloquent",
        "laravel",
        "subscription",
        "plan",
        "plans",
        "model",
        "saas"
    ],
    "homepage": "https://github.com/bezhanSalleh/laravel-plans",
    "license": "MIT",
    "authors": [
        {
            "name": "Bezhan Salleh",
            "role": "Maintainer"
        }, {
            "name": "Alex Renoki",
            "email": "rennokki@gmail.com",
            "homepage": "https://twitter.com/rennokki",
            "role": "Original Developer"
        }
    ],
    "require": {
        "doctrine/dbal": "^2.11.1",
        "laravel/framework": "^5.5.0|^5.6.0|^5.7.0|^5.8.0|^6.0|^7.0|^8.0",
        "stripe/stripe-php": "^6.11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2|^7.0|^8.5|^9.0",
        "orchestra/testbench": "^3.5.0|^3.6.0|^3.7.0|^3.8.0|^4.0|^5.0|^6.0",
        "orchestra/database": "~3.5.0|~3.6.0"
    },
    "autoload": {
        "psr-4": {
            "BezhanSalleh\\LaravelPlans\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BezhanSalleh\\LaravelPlans\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "BezhanSalleh\\LaravelPlans\\PlansServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
